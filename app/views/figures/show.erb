<h2>Figure</h2>
	
		<ul>
			<li><%= @figure.name %>
				<ul>
					<% @figure.titles.each do |title| %>
					<li><%= title.name %></li>
					<% end %>
				</ul>
				<ul>
					<% @figure.landmarks.each do |landmark| %>
					<p>Figure Landmarks</p>
					<li><%= landmark.name %></li>
					<% end %>
				</ul>
			</li>
		</ul>

		<button><a href="/figures/<%= @figure.id %>/edit">Edit</a></button>