<h2>Figures</h2>
	
	<% Figure.all.each do |figure| %>
		<ul>
			<li><%= figure.name %>
				<ul>
					<% figure.titles.each do |title| %>
					<li><%= title.name %></li>
					<% end %>
				</ul>
				<ul>
					<% figure.landmarks.each do |landmark| %>
					<p>Figure Landmarks</p>
					<li><%= landmark.name %></li>
					<% end %>
				</ul>

			</li>
		</ul>
	<% end %> 


<h2>Landmarks</h2>

<% Landmark.all.each do |landmark| %>
	<ul>
		<li><%= landmark.name %></li>
		<li><%= landmark.year_completed %></li>
	</ul>
<% end %> 